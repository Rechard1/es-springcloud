<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jwell56.security.cloud.service.asset.mapper.SysDeviceMonitorUnitMapper">

   <select id="getUnitIdList" resultType="java.lang.Integer">
      select unit_id from sys_device_monitor_unit
      where device_id = #{probeId}
   </select>

</mapper>